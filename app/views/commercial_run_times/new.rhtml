<%= render :partial => 'header' %>
<div id="content">
<center>
<h1>New Commercial Run Time</h1>

<% form_for(@commercial_run_time) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :minutes %><br />
    <%= f.text_field :minutes %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', commercial_run_times_path %>

</center>
<br/><br/>
</div>